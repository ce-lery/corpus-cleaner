{
    "original_folder_path":"../../results/dataset/original/",
    "base_folder_path":"../../results/dataset/",
    "results_folder_path":"../../results/dataset/cleaned/",
    "rejected_folder_path":"../../results/dataset/rejected/",
    "blacklist_folder_path":"../../results/dataset/blacklist/",
    "store_blacklist":false,
    "min_length":5,
    "max_length":500000,
    "accept_language":["__label__ja"],
    "store_rejected":true,
    "language_threshold":0.3,
    "perplexity_threshold":80000,
    "specific_phrases":[
        "このページは曖昧さ回避のためのページです。一つの語句が複数の意味・職能を有する場合の水先案内のために、異なる用法を一覧にしてあります。お探しの用語に一番近い記事を選んで下さい。このページへリンクしているページを見つけたら、リンクを適切な項目に張り替えて下さい。"
    ],
    "execute_sentence_segment":false,
    "use_control_character_remover":true,
    "use_normalizer":true,
    "use_url_remover":true,
    "use_emoji_remover":true,
    "use_special_character_remover":true,
    "use_quotes_remover":true,
    "use_length_filter":true,
    "use_zero_punctuation_filter":true,
    "use_language_filter":true,
    "use_noun_ratio_filter":true,
    "use_minhash_filter":false,
    "use_perplexity_filter":true
}